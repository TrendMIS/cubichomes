<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="project_project_view_inherit_form" model="ir.ui.view">
            <field name="name">project.project.view.inherit.form</field>
            <field name="model">project.project</field>
            <field name="inherit_id" ref="project.edit_project"/>
            <field name="arch" type="xml">

                <xpath expr="//group/field[@name='label_tasks']" position="before">
                    <field name="project_ref"/>
                </xpath>

            </field>
        </record>

    </data>
</odoo>